public class ScientificCalculator {
    double value1;
    double value2;
    double value3;


    ScientificCalculator() {
        this.value1= value1;
        this.value2= value2;
        this.value3= value3;

    }

    public double addition(double value1, double value2){

        value3= value1 + value2;

        return value3;
    }
    public double subtraction(double value1, double value2){

        value3= value1 - value2;

        return value3;
    }
    public double multiplication(double value1, double value2){

        value3= value1 * value2;

        return value3;
    }
    public double division(double value1, double value2){

        value3=value1/value2;

        return value3;
    }

    public double squareRoot(double value1){

        return Math.sqrt(value1);
    }
    public double power(double value1, double value2){

        value3 = Math.pow(value1,value2);

        return value3;
    }
    public double sinus (double value1){

        return Math.sin(Math.toRadians(value1));
    }
    public double cosinus (double value1){

        return Math.cos(Math.toRadians(value1));
    }
    public double remain (int value1, int value2){

        value3 = value1 % value2;
        return value3;
    }
}
